    "@/hc595-clear": {
      "attachments": [
        {
          "content": "struct State {\n};\n\n{{ GENERATED_CODE }}\n\nvoid evaluate(Context ctx) {\n    // The node responds only if there is an input pulse\n    if (!isInputDirty<input_CLR>(ctx)){\n    emitValue<output_OUT>(ctx, getValue<input_DEV>(ctx));\n        return;}\n\n    // Get a pointer to the class instance\n    auto sr = getValue<input_DEV>(ctx);\n    sr->setAllLow(); // set all pins LOW\n    emitValue<output_DONE>(ctx, 1);\n    emitValue<output_OUT>(ctx, sr);\n}\n",
          "encoding": "utf-8",
          "filename": "patch.cpp"
        }
      ],
