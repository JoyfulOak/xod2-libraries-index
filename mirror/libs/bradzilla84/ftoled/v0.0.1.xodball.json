        {
          "content": "{{#global}}\n#include <fonts/Arial14.h>\n{{/global}}\n\nstruct State {\n};\n\n{{ GENERATED_CODE }}\n\nvoid evaluate(Context ctx) {\n    // The node responds only if there is an input pulse\n    if (!isInputDirty<input_INIT>(ctx)){\n    emitValue<output_OUT>(ctx, getValue<input_DEV>(ctx));\n        return;}\n\n    // Get a pointer to the `OLED` class instance\n    auto Display = getValue<input_DEV>(ctx);\n\n    OLED_Colour ColourTXT = {getValue<input_RED>(ctx),getValue<input_GREEN>(ctx),getValue<input_BLUE>(ctx)};\n    OLED_Colour ColourBackground = {getValue<input_REDBackground>(ctx),getValue<input_GREENBackground>(ctx),getValue<input_BLUEBackground>(ctx)};\n    auto TXTString = getValue<input_String>(ctx);\n    String TXT = \"\";\n    for (auto it = TXTString.iterate(); it; ++it)\n        TXT = TXT + (*it);\n    \n    Display->selectFont(Arial14);\n    Display->drawString(getValue<input_XStart>(ctx),getValue<input_YStart>(ctx),(TXT),(ColourTXT),(ColourBackground));\n\n    // Pulse that module initialized successfully\n    emitValue<output_OUT>(ctx, Display);\n    emitValue<output_OK>(ctx, 1);\n}",
          "encoding": "utf-8",
          "filename": "patch.cpp"
        }
