      "path": "@/read-keys",
      "description": "Read user programmable buttons of Wio Terminal.",
      "attachments": [
        {
          "filename": "patch.cpp",
          "encoding": "utf8",
          "content": "#pragma XOD evaluate_on_pin disable\n#pragma XOD evaluate_on_pin enable input_UPD\n\nnode {\n    void evaluate(Context ctx) {\n        if (!isInputDirty<input_UPD>(ctx))\n            return;\n\n        ::pinMode(WIO_KEY_A, INPUT_PULLUP);\n        ::pinMode(WIO_KEY_B, INPUT_PULLUP);\n        ::pinMode(WIO_KEY_C, INPUT_PULLUP);\n        emitValue<output_A>(ctx, ::digitalRead(WIO_KEY_A));\n        emitValue<output_B>(ctx, ::digitalRead(WIO_KEY_B));\n        emitValue<output_C>(ctx, ::digitalRead(WIO_KEY_C));\n        emitValue<output_DONE>(ctx, 1);\n    }\n}\n"
        }
      ]
    },
