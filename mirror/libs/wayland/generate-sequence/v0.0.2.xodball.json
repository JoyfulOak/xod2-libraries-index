        }
      },
      "path": "@/generate-sequence",
      "description": "Generate a linear sequence of numbers.",
      "attachments": [
        {
          "filename": "patch.cpp",
          "encoding": "utf-8",
          "content": "\nnode {\n    // Internal state variables defined at this level persists across evaluations\n    Number n;\n    Number start;\n    Number stop;\n    Number step;\n    bool update;\n\n    void evaluate(Context ctx) {\n        if(isInputDirty<input_UPD>(ctx)) {\n            start = getValue<input_Start>(ctx);\n            stop = getValue<input_Stop>(ctx);\n            step = getValue<input_Step>(ctx);\n            n = start;\n            update = true;\n        }\n\n        if (update) {\n            if (n <= stop) {\n                emitValue<output_N>(ctx, n);\n                emitValue<output_Next>(ctx, 1);\n                n += step;\n                setImmediate();\n            }\n            else\n            {\n                update = false;\n                emitValue<output_Done>(ctx, 1);\n            }\n        }\n        \n    }\n}\n"
        }
      ]
    },
