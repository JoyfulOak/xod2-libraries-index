    "@/scd4x-device": {
      "attachments": [
        {
          "filename": "patch.cpp",
          "encoding": "utf-8",
          "content": "#pragma XOD require \"https://github.com/sparkfun/SparkFun_SCD4x_Arduino_Library\"\n#include <Wire.h>\n#include \"SparkFun_SCD4x_Arduino_Library.h\"\n\n\nnode {\n\n        meta {\n        // Define our custom type as a pointer on the class instance.\n        using Type = SCD4x*;\n    }\n\n    // Create an object of class Adafruit_TSL2591\n    SCD4x mySensor = SCD4x();\n\n\n    \n    // Internal state variables defined at this level persists across evaluations\n    Number foo;\n    uint8_t bar = 5;\n\n    void evaluate(Context ctx) {\n        bar += 42;\n\n        if (isSettingUp()) {\n            // This run once\n            foo = (Number)(bar + 1);\n        }\n\n        Wire.begin();\n        mySensor.begin();\n        \n        emitValue<output_OUT>(ctx, &mySensor);\n    }\n}\n"
        }
      ],
