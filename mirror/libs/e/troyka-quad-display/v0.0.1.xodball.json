    "@/quad-display": {
      "attachments": [
        {
          "content": "#pragma XOD require \"https://github.com/amperka/QuadDisplay2\"\n\n{{#global}}\n#include <QuadDisplay2.h>\n{{/global}}\n\nstruct State {\n    uint8_t mem[sizeof(QuadDisplay)];\n};\n\nusing Type = QuadDisplay*;\n\n{{ GENERATED_CODE }}\n\nvoid evaluate(Context ctx) {\n    if (!isSettingUp()) return;\n\n    auto state = getState(ctx);\n    auto pinCS = getValue<input_CS>(ctx);\n\n    Type qd = new (state->mem) QuadDisplay(pinCS);\n    qd->begin();\n\n    emitValue<output_DEV>(ctx, qd);\n}\n",
          "encoding": "utf8",
          "filename": "patch.cpp"
        }
      ],
