    "@/true-to-pulse": {
      "attachments": [
        {
          "content": "struct State {\n};\n\n{{ GENERATED_CODE }}\n\nvoid evaluate(Context ctx) {\n    \n  if (getValue<input_bool>(ctx)) {\n    emitValue<output_pulse>(ctx, 1);\n    }\n}\n\n",
          "encoding": "utf8",
          "filename": "patch.cpp"
        }
      ],
      "description": "Convert (each) true into a pulse. Compare with cast-boolean-to-pulse",
