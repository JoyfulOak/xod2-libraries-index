      "path": "@/calculate",
      "attachments": [
        {
          "filename": "patch.cpp",
          "encoding": "utf8",
          "content": "\nstruct State {\n};\n\n{{ GENERATED_CODE }}\n\nvoid evaluate(Context ctx) {\n\n    byte lsb = getValue<input_LSB>(ctx);\n    byte msb = getValue<input_MSB>(ctx);\n\n    int exp = (msb & 0xF0) >> 4;\n    int man = ((msb & 0x0F) << 4) | (lsb & 0x0F);\n\n    float lum = (2 << exp) * man * 0.045;\n\n    emitValue<output_LUM>(ctx, lum);\n}\n"
        }
      ]
    },
