    "@/receive-one-byte": {
      "attachments": [
        {
          "filename": "patch.cpp",
          "encoding": "utf-8",
          "content": "#pragma XOD require \"https://github.com/espressif/arduino-esp32\"\n\n#include <WiFiClient.h>\n\nnode {\n    void evaluate(Context ctx) {\n        if (!isInputDirty<input_READ>(ctx))\n            return;\n\n        auto req = getValue<input_REQ>(ctx);\n\n        WiFiClient* client = req.http->getStreamPtr();\n        int r = client->read();\n        emitValue<output_CHAR>(ctx, (uint8_t)r);\n        emitValue<output_DONE>(ctx, 1);\n    }\n}\n"
        }
      ],
