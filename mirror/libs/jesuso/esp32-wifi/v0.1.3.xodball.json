    "@/add-headers": {
      "attachments": [
        {
          "filename": "patch.cpp",
          "encoding": "utf-8",
          "content": "\nnode {\n    void evaluate(Context ctx) {\n        if (!isInputDirty<input_INIT>(ctx))\n            return;\n\n        auto req = getValue<input_REQ>(ctx);\n\n        // Add headers\n        XString __hdrs = getValue<input_HDRS>(ctx);\n        char _hdrs[length(__hdrs) + 1] = { 0 };\n        dump(__hdrs, _hdrs);\n        String hdrs = String(_hdrs);\n\n        size_t pos_from = 0;\n        while (true) {\n            size_t pos_to = hdrs.indexOf(\",\", pos_from);\n            size_t pos_delim = hdrs.indexOf(\":\", pos_from);\n\n            if (pos_to == -1) pos_to = hdrs.length();\n\n            String key = hdrs.substring(pos_from, pos_delim);\n            String val = hdrs.substring(pos_delim + 1, pos_to);\n\n            if (key.length() > 0 && val.length() > 0) {\n                req.http->addHeader(key, val);\n            }\n\n            if (pos_to == hdrs.length()) break;\n            pos_from = pos_to + 1;\n        }\n\n        emitValue<output_DONE>(ctx, 1);\n        emitValue<output_REQU0027>(ctx, req);\n    }\n}\n"
        }
      ],
