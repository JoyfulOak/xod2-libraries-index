    "@/esp32-request": {
      "attachments": [
        {
          "filename": "patch.cpp",
          "encoding": "utf-8",
          "content": "#pragma XOD require \"https://github.com/espressif/arduino-esp32\"\n\n#include <HTTPClient.h>\n\nnode {\n    uint8_t mem[sizeof(HTTPClient)];\n    HTTPClient* http;\n\n    meta {\n      struct Type {\n          HTTPClient* http;\n      };\n    }\n\n    void evaluate(Context ctx) {\n        if (!isInputDirty<input_INIT>(ctx))\n            return;\n\n        auto url = getValue<input_URL>(ctx);\n        char _url[length(url) + 1] = { 0 };\n        dump(url, _url);\n\n        http = new (mem) HTTPClient();\n        Type obj = { http };\n        http->begin(_url);\n\n        emitValue<output_REQ>(ctx, obj);\n        emitValue<output_DONE>(ctx, 1);\n    }\n}\n"
        }
      ],
