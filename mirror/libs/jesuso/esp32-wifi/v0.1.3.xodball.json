          "filename": "patch.cpp",
          "encoding": "utf-8",
          "content": "#pragma XOD require \"https://github.com/espressif/arduino-esp32\"\n#include <WiFi.h>\n\nnode {\n    void evaluate(Context ctx) {\n        if (!isInputDirty<input_INIT>(ctx))\n            return;\n\n        XString _method = getValue<input_METH>(ctx);\n        char method[length(_method) + 1] = { 0 };\n        dump(_method, method);\n\n        auto req = getValue<input_REQ>(ctx);\n\n        XString _body = getValue<input_BODY>(ctx);\n        char body[length(_body) + 1] = { 0 };\n        dump(_body, body);\n\n        int httpResponseCode = 0;\n        if (length(_body) > 0) {\n            httpResponseCode = req.http->sendRequest(method, body);\n        } else {\n            httpResponseCode = req.http->sendRequest(method);\n        }\n\n        emitValue<output_STATUS>(ctx, httpResponseCode);\n        emitValue<output_REQU0027>(ctx, req);\n        emitValue<output_DONE>(ctx, 1);\n    }\n}\n"
        }
