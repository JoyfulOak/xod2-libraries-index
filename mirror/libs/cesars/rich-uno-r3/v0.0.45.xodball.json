    "@/knob": {
      "attachments": [
        {
          "content": "#pragma XOD require \"https://github.com/Cesar-S/RichUNO\"\n#include <RichUNOKnob.h>\n\n#define KNOB_PIN A0\nKnob knob(KNOB_PIN);\n\nnode {\n\n    void evaluate(Context ctx) {\n\n        if (!isInputDirty<input_UPD>(ctx))return;\n\n        auto pot = ::analogRead(A0) / 1023.;\n        auto old_val = getValue<output_VAL>(ctx);\n        auto sMin = 0;\n        auto sMax = 1;\n        auto tMin = 0;\n        auto tMax = getValue<input_MAX>(ctx);\n        tMax = tMax + 0.1;\n        auto k = (pot - sMin) / (sMax - sMin);\n        auto val = isnan(pot) ? pot : tMin + k * (tMax - tMin);\n\n        switch((int)getValue<input_SEL_OUT>(ctx)){\n            case 1:\n                emitValue<output_VAL>(ctx, floor(val));\n                break;\n            case 2:\n                int angle = knob.getAngle();\n                emitValue<output_VAL>(ctx, angle);\n                break;\n            default:\n                emitValue<output_VAL>(ctx, pot);\n                break;\n        }\n    }\n}\n",
          "encoding": "utf8",
          "filename": "patch.cpp"
        }
      ],
