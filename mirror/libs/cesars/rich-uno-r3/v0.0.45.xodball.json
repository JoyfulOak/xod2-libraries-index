      "attachments": [
        {
          "content": "#pragma XOD evaluate_on_pin enable input_UPD\n\nnode {\n\n    static_assert(isValidDigitalPort(constant_input_TCH1), \"must be a valid digital port\");\n    static_assert(isValidDigitalPort(constant_input_TCH2), \"must be a valid digital port\");\n    static_assert(isValidDigitalPort(constant_input_TCH3), \"must be a valid digital port\");\n    static_assert(isValidDigitalPort(constant_input_TCH4), \"must be a valid digital port\");\n    void evaluate(Context ctx) {\n\n        if (!isInputDirty<input_UPD>(ctx)) return;\n        ::pinMode(constant_input_TCH1, INPUT);\n        ::pinMode(constant_input_TCH2, INPUT);\n        ::pinMode(constant_input_TCH3, INPUT);\n        ::pinMode(constant_input_TCH4, INPUT);\n        emitValue<output_1UD83DUDC46>(ctx, ::digitalRead(constant_input_TCH1));\n        emitValue<output_2UD83DUDC46>(ctx, ::digitalRead(constant_input_TCH2));\n        emitValue<output_3UD83DUDC46>(ctx, ::digitalRead(constant_input_TCH3));\n        emitValue<output_4UD83DUDC46>(ctx, ::digitalRead(constant_input_TCH4));\n        emitValue<output_TICK>(ctx, 1);\n    }\n}\n",
          "encoding": "utf8",
          "filename": "patch.cpp"
