    "@/rich-touch": {
      "attachments": [
        {
          "content": "#pragma XOD evaluate_on_pin disable\n#pragma XOD evaluate_on_pin enable input_UPD\n#pragma XOD error_raise enable\n\nstruct State {\n};\n\n{{ GENERATED_CODE }}\n\nvoid evaluate(Context ctx) {\n    if (!isInputDirty<input_UPD>(ctx))\n        return;\n\n    const uint8_t port1 = getValue<input_TCH1>(ctx);\n    const uint8_t port2 = getValue<input_TCH2>(ctx);\n    const uint8_t port3 = getValue<input_TCH3>(ctx);\n    const uint8_t port4 = getValue<input_TCH4>(ctx);\n    if (!isValidDigitalPort(port1)) {\n        raiseError(ctx);\n        return;\n    }\n    if (!isValidDigitalPort(port2)) {\n        raiseError(ctx);\n        return;\n    }\n    if (!isValidDigitalPort(port3)) {\n        raiseError(ctx);\n        return;\n    }\n    if (!isValidDigitalPort(port4)) {\n        raiseError(ctx);\n        return;\n    }\n    ::pinMode(port1, INPUT);\n    ::pinMode(port2, INPUT);\n    ::pinMode(port3, INPUT);\n    ::pinMode(port4, INPUT);\n    emitValue<output_1UD83DUDC46>(ctx, ::digitalRead(port1));\n    emitValue<output_2UD83DUDC46>(ctx, ::digitalRead(port2));\n    emitValue<output_3UD83DUDC46>(ctx, ::digitalRead(port3));\n    emitValue<output_4UD83DUDC46>(ctx, ::digitalRead(port4));\n    emitValue<output_TICK>(ctx, 1);\n}\n",
          "encoding": "utf8",
          "filename": "patch.cpp"
        }
      ],
