    "@/max31855k-dev": {
      "attachments": [
        {
          "content": "#pragma XOD require \"https://github.com/Cesar-S/SparkFun_MAX31855K_Thermocouple_Breakout_Arduino_Library\"\n#pragma XOD error_raise enable\n#pragma XOD error_catch enable\n\n#include <SparkFunMAX31855k.h>\n\nnode {\n\n    static_assert(isValidDigitalPort(constant_input_CS), \"must be a valid digital port\");\n    static_assert(isValidDigitalPort(constant_input_VCC), \"must be a valid digital port\");\n    static_assert(isValidDigitalPort(constant_input_GND), \"must be a valid digital port\");\n\n\n    float temperature;\n    uint8_t mem[sizeof(SparkFunMAX31855k)];\n\n    void evaluate(Context ctx) {\n        if (isSettingUp()) {auto max = new (mem) SparkFunMAX31855k(constant_input_CS, constant_input_VCC, constant_input_GND); return;}\n\n        if(!isInputDirty<input_UPD>(ctx)) return;\n        auto max = reinterpret_cast<SparkFunMAX31855k*>(mem);\n\n        int8_t mode = getValue<input_MODE>(ctx);\n\n        switch(mode){\n            case 1: temperature = max->readTempC();\n                if (!isnan(temperature)) {emitValue<output_Temp_UD83CUDF21UFE0F>(ctx, temperature);}\n                else{raiseError(ctx);}\n                break;\n            case 2: temperature = max->readTempF();\n                if (!isnan(temperature)) {emitValue<output_Temp_UD83CUDF21UFE0F>(ctx, temperature);}\n                else{raiseError(ctx);}\n                break;\n            case 3: temperature = max->readTempK();\n                if (!isnan(temperature)) {emitValue<output_Temp_UD83CUDF21UFE0F>(ctx, temperature);}\n                else{raiseError(ctx);}\n                break;\n            case 4: temperature = max->readTempR();\n                if (!isnan(temperature)) {emitValue<output_Temp_UD83CUDF21UFE0F>(ctx, temperature);}\n                else{raiseError(ctx);}\n                break;\n            default: temperature = max->readCJT();\n                if (!isnan(temperature)) {emitValue<output_Temp_UD83CUDF21UFE0F>(ctx, temperature);}\n                else{raiseError(ctx);}\n                break;\n        }\n    }\n}\n",
          "encoding": "utf8",
          "filename": "patch.cpp"
        }
      ],
